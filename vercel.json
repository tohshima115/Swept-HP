{
  "rewrites": [
    {
      "source": "/attachart",
      "has": [
        {
          "type": "header",
          "key": "User-Agent",
          "value": ".*(Twitterbot|facebookexternalhit|line-poker|Discordbot).*"
        }
      ],
      "destination": "/api/ogp"
    },
    {
      "source": "/:path((?!.*\\.).*)",
      "destination": "/index.html"
    }
  ]
} 